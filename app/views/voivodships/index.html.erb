<h1>Listing voivodships</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @voivodships.each do |voivodship| %>
      <tr>
        <td><%= voivodship.name %></td>
        <td><%= link_to 'Show', voivodship %></td>
        <td><%= link_to 'Edit', edit_voivodship_path(voivodship) %></td>
        <td><%= link_to 'Destroy', voivodship, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Voivodship', new_voivodship_path %>
